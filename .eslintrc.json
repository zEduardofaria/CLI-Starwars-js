{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "airbnb-base"
  ],
  "rules": {
    "no-restricted-syntax": "off",
    "no-await-in-loop": "off",
    "prefer-destructuring": "off",
    "no-shadow": "off"
  }
}