{
  "name": "starwars",
  "description": "This is a cool CLI for Smarttbot",
  "version": "0.0.0",
  "author": "Eduardo Faria",
  "bin": {
    "starwars": "./bin/run"
  },
  "bugs": "https://github.com/CLI-Starwars-js/starwars/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "swapi-wrapper": "^1.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/CLI-Starwars-js/starwars",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "starwars",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "CLI-Starwars-js/starwars",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "readme": "oclif-dev readme --multi",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "starwars": "./bin/run"
  }
}
